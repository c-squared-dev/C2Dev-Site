{
  "extends": ["wesbos"],
  "rules": {
    "no-console": 2,
    "import/no-unresolved": 0,
    "react/display-name": 0,
    "quotes": "off",
    "prettier/prettier": [
      "warn",
      {
        "trailingComma": "off",
        "singleQuote": false,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}
